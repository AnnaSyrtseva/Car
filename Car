public class Car {

    Model model;
    Color color;
    Speed speed;

    int currentSpeed;
    int y;



    public Car(Model model, Color color, Speed speed) {
        this.model = model;
        this.color = color;
        this.speed = speed;
    }

    @Override
    public String toString() {
        return "Car{" +
                "model=" + model +
                ", color=" + color +
                ", speed=" + speed +
                '}';
    }

    int accelerate(int s) {
        currentSpeed = s+5;
        return currentSpeed;

    }

    int slowDown(int i) {
        y = i -5; 
        return y;

    }
}
